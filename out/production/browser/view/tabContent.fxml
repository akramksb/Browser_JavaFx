<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<?import javafx.scene.image.Image?>
<AnchorPane fx:id="tabContent" minHeight="0.0" minWidth="0.0" prefHeight="494.0" prefWidth="916.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TabController">
   <children>
      <WebView fx:id="webView" layoutY="53.0" prefHeight="186.0" prefWidth="388.0" AnchorPane.bottomAnchor="-2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="53.0" />
      <TextField fx:id="barRecherche" onAction="#loadPage" prefHeight="26.0" prefWidth="916.0" />
      <Button fx:id="buttonGo" layoutY="26.0" mnemonicParsing="false" onAction="#loadPage" text="Go" />
      <Button fx:id="buttonRe" layoutX="32.0" layoutY="26.0" mnemonicParsing="false" onAction="#refreshPage" text="Re" />
      <Button fx:id="buttonZoonOut" layoutX="62.0" layoutY="26.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
      <Button fx:id="buttonZoonIn" layoutX="83.0" layoutY="26.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
      <Button fx:id="buttonHistory" layoutX="718.0" layoutY="26.0" mnemonicParsing="false" onAction="#displayHistory" >
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <Image url="@icons/icons8-activity-history-48.png"/>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="buttongoBack" layoutX="845.0" layoutY="26.0" mnemonicParsing="false" onAction="#goBack" text="&lt;" />
      <Button fx:id="buttongoFront" layoutX="877.0" layoutY="26.0" mnemonicParsing="false" onAction="#goForward" text="&gt;" />
      <Button fx:id="buttonPrint" layoutX="634.0" layoutY="26.0" mnemonicParsing="false" onAction="#print">
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" >
               <Image url="@icons/icons8-print-48.png"/>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
